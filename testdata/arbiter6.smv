MODULE main
	VAR// [TA] if we change modules order from [s;e] to [e;s], the computation complexity will increase much higher
		s : sys(e.r1, e.r2, e.r3, e.r4, e.r5, e.r6);
		e : env(s.g1, s.g2, s.g3, s.g4, s.g5, s.g6);

MODULE env(g1, g2, g3, g4, g5, g6)
	VAR
		r1 : boolean;
		r2 : boolean;
		r3 : boolean;
		r4 : boolean;
		r5 : boolean;
		r6 : boolean;
	ASSIGN
		init(r1) := FALSE;
		init(r2) := FALSE;
		init(r3) := FALSE;
		init(r4) := FALSE;
		init(r5) := FALSE;
		init(r6) := FALSE;
	TRANS
		((r1 != g1) -> (r1 <-> next(r1))) &
		((r2 != g2) -> (r2 <-> next(r2))) &
		((r3 != g3) -> (r3 <-> next(r3))) &
		((r4 != g4) -> (r4 <-> next(r4))) &
		((r5 != g5) -> (r5 <-> next(r5))) &
		((r6 != g6) -> (r6 <-> next(r6)));
	JUSTICE
		!(r1 & g1);
		!(r2 & g2);
		!(r3 & g3);
		!(r4 & g4);
		!(r5 & g5);
		!(r6 & g6);

MODULE sys(r1, r2, r3, r4, r5, r6)
	VAR
		g1 : boolean;
		g2 : boolean;
		g3 : boolean;
		g4 : boolean;
		g5 : boolean;
		g6 : boolean;
	ASSIGN
		init(g1) := FALSE;
		init(g2) := FALSE;
		init(g3) := FALSE;
		init(g4) := FALSE;
		init(g5) := FALSE;
		init(g6) := FALSE;
	TRANS
		((r1 <-> g1) -> (g1 <-> next(g1))) &
		((r2 <-> g2) -> (g2 <-> next(g2))) &
		((r3 <-> g3) -> (g3 <-> next(g3))) &
		((r4 <-> g4) -> (g4 <-> next(g4))) &
		((r5 <-> g5) -> (g5 <-> next(g5))) &
		((r6 <-> g6) -> (g6 <-> next(g6))) &
		!(next(g1) & next(g2)) &
		!(next(g1) & next(g3)) &
		!(next(g1) & next(g4)) &
		!(next(g1) & next(g5)) &
		!(next(g1) & next(g6)) &
		!(next(g2) & next(g3)) &
		!(next(g2) & next(g4)) &
		!(next(g2) & next(g5)) &
		!(next(g2) & next(g6)) &
		!(next(g3) & next(g4)) &
		!(next(g3) & next(g5)) &
		!(next(g3) & next(g6)) &
		!(next(g4) & next(g5)) &
		!(next(g4) & next(g6)) &
		!(next(g5) & next(g6));
	JUSTICE
		r1 <-> g1;
		r2 <-> g2;
		r3 <-> g3;
		r4 <-> g4;
		r5 <-> g5;
		r6 <-> g6;